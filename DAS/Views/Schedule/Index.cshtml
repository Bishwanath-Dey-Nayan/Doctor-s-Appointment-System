@model IEnumerable<DAS.Schedule>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout_Doctor.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CreateSchedule")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Chamber.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Doctor.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstAppointmentFee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NextMeetingFee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SlotNo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Chamber.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Doctor.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstAppointmentFee)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NextMeetingFee)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SlotNo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
